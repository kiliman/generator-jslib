    "@babel/preset-env": "^7.0.0",
    "chai": "^4.1.2",
    "esm": "^3.0.82",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "files": ["dist"]
