{
  "name": "<%= libname %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "readmeFilename": "README.md",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "repository": {
    "url": "<%= repositoryUrl %>",
    "type": "<%= repositoryType %>"
  },
  "license": "<%= license %>",
  "engines": {
    "node": ">= 0.10.0"
  },
